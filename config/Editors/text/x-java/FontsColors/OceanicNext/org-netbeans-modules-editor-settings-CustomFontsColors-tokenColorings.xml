<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors
PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN"
       "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
 <fontcolor name="mod-abstract" default="default"/>
 <fontcolor name="mod-annotation-type-declaration" default="identifier">
  <font name="Courier New" size="13" style="bold"/>
 </fontcolor>
 <fontcolor name="mod-annotation-type" default="identifier"/>
 <fontcolor name="character" default="char"/>
 <fontcolor name="character-escape" default="character">
  <font name="Source Code Pro" size="13" style="bold"/>
 </fontcolor>
 <fontcolor name="mod-class-declaration" default="identifier" foreColor="ffFAC863">
  <font name="Courier New" size="13" style="bold"/>
 </fontcolor>
 <fontcolor name="mod-class" default="identifier"/>
 <fontcolor name="comment" default="comment"/>
 <fontcolor name="mod-constructor-declaration" default="identifier">
  <font name="Courier New" size="13" style="bold"/>
 </fontcolor>
 <fontcolor name="mod-constructor" default="identifier"/>
 <fontcolor name="mod-deprecated" default="default" strikeThrough="ff000000"/>
 <fontcolor name="mod-enum-declaration" default="identifier">
  <font name="Source Code Pro" size="13" style="bold"/>
 </fontcolor>
 <fontcolor name="mod-enum" default="identifier"/>
 <fontcolor name="mod-field-declaration" default="field" foreColor="ffCDD3DE"/>
 <fontcolor name="mod-field" default="field" foreColor="ffCDD3DE"/>
 <fontcolor name="identifier" default="identifier"/>
 <fontcolor name="mod-interface-declaration" default="identifier"/>
 <fontcolor name="mod-interface" default="identifier"/>
 <fontcolor name="javadoc-first-sentence" default="comment">
  <font name="Courier New" size="13" style="bold"/>
 </fontcolor>
 <fontcolor name="html-tag" default="comment">
  <font name="Source Code Pro" size="13" style="bold"/>
 </fontcolor>
 <fontcolor name="javadoc-identifier" default="comment" foreColor="ff65737e"/>
 <fontcolor name="javadoc-tag" default="comment">
  <font style="bold"/>
 </fontcolor>
 <fontcolor name="keyword" default="keyword"/>
 <fontcolor name="keyword-directive" default="keyword"/>
 <fontcolor name="literal" default="keyword" foreColor="ffF99157"/>
 <fontcolor name="mod-local-variable" default="identifier"/>
 <fontcolor name="mod-local-variable-declaration" default="identifier"/>
 <fontcolor name="mod-method-declaration" default="method" foreColor="ffD8DEE9"/>
 <fontcolor name="mod-method" default="method"/>
 <fontcolor name="number" default="number"/>
 <fontcolor name="operator" default="operator"/>
 <fontcolor name="mod-package-private" default="default"/>
 <fontcolor name="mod-parameter-declaration" default="identifier"/>
 <fontcolor name="mod-parameter" default="identifier"/>
 <fontcolor name="mod-private" default="default"/>
 <fontcolor name="mod-protected" default="default"/>
 <fontcolor name="mod-public" default="default"/>
 <fontcolor name="separator" default="separator" foreColor="ff5FB3B3"/>
 <fontcolor name="mod-static" default="default">
  <font name="Courier New" size="13" style="bold+italic"/>
 </fontcolor>
 <fontcolor name="string" default="string"/>
 <fontcolor name="string-escape" default="string">
  <font name="Courier New" size="13" style="bold"/>
 </fontcolor>
 <fontcolor name="mod-unused" default="default" waveUnderlined="ff999999"/>
 <fontcolor name="mark-occurrences" default="default" foreColor="ff1B2B34" bgColor="ffFAC863"/>
</fontscolors>
